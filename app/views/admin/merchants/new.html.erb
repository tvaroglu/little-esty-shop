<h1>New Admin Merchant</h1>
<br>
<div align="center">
  <%= form_with url: admin_merchants_path, method: :post, local: true, :html => { :class => 'card navbar-custom card-body', :style => 'width: 18rem;' } do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <div style="padding-top:10px;">
      <%= f.submit 'Create', :class => "btn btn-outline-dark navbar-custom" %>
    </div>
  <% end %>
</div>
